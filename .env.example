# Environment variables example
# Public Cloudinary cloud name (safe to expose in client builds)
NEXT_PUBLIC_CLOUDINARY_CLOUD_NAME=

# Server-only Cloudinary credentials (do NOT add these to client-side code)
CLOUDINARY_API_KEY=
CLOUDINARY_API_SECRET=

# Twilio / WhatsApp (if used)
TWILIO_ACCOUNT_SID=
TWILIO_AUTH_TOKEN=
TWILIO_WHATSAPP_FROM=

# Other env vars (add as needed)
